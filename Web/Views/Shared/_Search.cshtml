@model string;

@if(Model != "Account")
{
    <form class="form-inline my-2 my-lg-0" asp-controller="@Model" asp-action="Index" style="padding-right: 25px;">
        <input name="searchString" type="text" class="form-control" placeholder="Search for a @Model ..." aria-label="Recipient's username" aria-describedby="button-addon2">
        <button class="btn btn-outline-success my-2 my-sm-0" type="submit"><i class="bi bi-search"></i></button>
    </form>
}
